{
  "name": "alt-three/segment",
  "description": "A Segment bridge for Laravel",
  "keywords": [
    "segment",
    "bridge",
    "segment.com",
    "segment.io",
    "api",
    "Alt Three"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Alt Three",
      "email": "support@alt-three.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/config": "^9.0 || ^10.0",
    "illuminate/support": "^9.0 || ^10.0",
    "segmentio/analytics-php": "^3.6.0"
  },
  "require-dev": {
    "graham-campbell/analyzer": "^4.0",
    "graham-campbell/testbench": "^6.0",
    "phpunit/phpunit": "^9.0 || ^10.0"
  },
  "autoload": {
    "psr-4": {
      "AltThree\\Segment\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Synchro\\Segment\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "laravel": {
      "providers": [
        "AltThree\\Segment\\SegmentServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
